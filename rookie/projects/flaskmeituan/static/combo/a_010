;M.add("uix-collapse",function(e){function l(l,s){if(l=e.one(l)){var r={group:".uix-collapse",trigger:".uix-collapse__trigger",content:".uix-collapse__content",openClass:"uix-collapse--open",triggerExclude:"",multiple:!1,triggerEvent:"click"};s=e.mix(s||{},r),l.delegate(s.triggerEvent,function(e){var r=this.ancestor(s.group);if(r&&(!s.triggerExclude||!e.target.hasClass(s.triggerExclude))){var a=r.hasClass(s.openClass);"hover"===s.triggerEvent&&a||(s.multiple||l.all(s.group).removeClass(s.openClass),a?r.removeClass(s.openClass):r.addClass(s.openClass))}},s.trigger)}}e.namespace("mt.uix"),e.mt.uix.Collapse=l},"1.0.0",{requires:["mt-base","node"]});
