;M.add("system-message",function(s){s.mt.ComponentHub.attach("system-message",function(){var e=this.getScope(),t=s.mt.flux,m=s.mt.require("www-constants-common");e.$default({messages:[],type:"",message:"",msgId:"",isDisplay:function(){return e.messages&&e.messages.length},hide:function(){t.dispatch({actionType:m.HIDE_MSG,id:e.msgId})}}),e.$watch("messages",function(s){var t=s&&s.length&&s[s.length-1];t&&(e.message=t.msg,e.type=t.type,e.msgId=t.id)})})},"1.0.0",{requires:["www-base","mt-flux","www-constants-common","mt-scope"]});
